menu "Fast path Tunnel options"

config MCORE_XIN4
	bool "IPv4/IPv6 over IPv4 support"
	depends on MCORE_IP
	default y

config MCORE_XIN4_HASH_ORDER
	int "Tunnel hash table size"
	depends on MCORE_XIN4
	default 4

config MCORE_XIN6
	bool "IPv4/IPv6 support over IPv6 support"
	depends on MCORE_IPV6
	default y

config MCORE_XIN6_HASH_ORDER
	int "Tunnel IPv6 hash table size"
	depends on MCORE_XIN6
	default 4

endmenu
